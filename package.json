{
  "name": "markdown2png",
  "version": "1.8.0",
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "start": "pnpm vite",
    "dev": "vite",
    "build": "run-p type-check build-only && pnpm generate:sitemap",
    "deploy": "bash deploy.sh",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "analyze": "vite-bundle-visualizer"
  },
  "dependencies": {
    "@headlessui/tailwindcss": "^0.2.0",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.1.1",
    "@silvia-odwyer/photon": "^0.3.2",
    "@zumer/snapdom": "^1.9.7",
    "html2canvas": "^1.4.1",
    "marked": "^10.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-wasm": "^3.2.2",
    "vue-tsc": "^1.8.22"
